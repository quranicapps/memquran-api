{
  "ContentDeliverySettings": {
    "Type": "Local", // Unknown, Local, JsDelivr, JsDelivrFallback
    "CachingSettings": {
      "CacheType": "Memory", // None, Memory, Redis
      "InitialCachingEnabled": true,
      "SlidingExpiration": false,
      "CacheDurationTimeSpan": "02:00:00:00"
    }
  },
  "Clients": {
    "JsDelivrService": {
      "Enabled": true,
      "BaseUrl": "https://cdn.jsdelivr.net",
      "Version": "v0.0.6",
      "DefaultTimeout": "00:00:05",
      "IncrementalBackoffFactorSeconds": 5,
      "UserName": "",
      "Password": ""
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "System.Net.Http": "Warning"
    },
    "Console": {
      "FormatterName": "CustomTimePrefixingFormatter",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.ffff ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "AllowedHosts": "*"
}
