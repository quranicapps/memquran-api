{"SurahNumber":100,"HasBismillah":true,"Verses":[{"SurahNumber":100,"VerseNumber":1,"Text":"وَٱلۡعَٰدِيَٰتِ ضَبۡحٗا","End":"١"},{"SurahNumber":100,"VerseNumber":2,"Text":"فَٱلۡمُورِيَٰتِ قَدۡحٗا","End":"٢"},{"SurahNumber":100,"VerseNumber":3,"Text":"فَٱلۡمُغِيرَٰتِ صُبۡحٗا","End":"٣"},{"SurahNumber":100,"VerseNumber":4,"Text":"فَأَثَرۡنَ بِهِۦ نَقۡعٗا","End":"٤"},{"SurahNumber":100,"VerseNumber":5,"Text":"فَوَسَطۡنَ بِهِۦ جَمۡعًا","End":"٥"},{"SurahNumber":100,"VerseNumber":6,"Text":"إِنَّ ٱلۡإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٞ","End":"٦"},{"SurahNumber":100,"VerseNumber":7,"Text":"وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٞ","End":"٧"},{"SurahNumber":100,"VerseNumber":8,"Text":"وَإِنَّهُۥ لِحُبِّ ٱلۡخَيۡرِ لَشَدِيدٌ","End":"٨"},{"SurahNumber":100,"VerseNumber":9,"Text":"۞أَفَلَا يَعۡلَمُ إِذَا بُعۡثِرَ مَا فِي ٱلۡقُبُورِ","End":"٩"},{"SurahNumber":100,"VerseNumber":10,"Text":"وَحُصِّلَ مَا فِي ٱلصُّدُورِ","End":"١٠"},{"SurahNumber":100,"VerseNumber":11,"Text":"إِنَّ رَبَّهُم بِهِمۡ يَوۡمَئِذٖ لَّخَبِيرُۢ","End":"١١"}],"Translations":[]}