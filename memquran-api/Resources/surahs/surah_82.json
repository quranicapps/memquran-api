{"SurahNumber":82,"HasBismillah":true,"Verses":[{"SurahNumber":82,"VerseNumber":1,"Text":"إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ","End":"١"},{"SurahNumber":82,"VerseNumber":2,"Text":"وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ","End":"٢"},{"SurahNumber":82,"VerseNumber":3,"Text":"وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ","End":"٣"},{"SurahNumber":82,"VerseNumber":4,"Text":"وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ","End":"٤"},{"SurahNumber":82,"VerseNumber":5,"Text":"عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ","End":"٥"},{"SurahNumber":82,"VerseNumber":6,"Text":"يَـٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ","End":"٦"},{"SurahNumber":82,"VerseNumber":7,"Text":"ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ","End":"٧"},{"SurahNumber":82,"VerseNumber":8,"Text":"فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ","End":"٨"},{"SurahNumber":82,"VerseNumber":9,"Text":"كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ","End":"٩"},{"SurahNumber":82,"VerseNumber":10,"Text":"وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ","End":"١٠"},{"SurahNumber":82,"VerseNumber":11,"Text":"كِرَامٗا كَٰتِبِينَ","End":"١١"},{"SurahNumber":82,"VerseNumber":12,"Text":"يَعۡلَمُونَ مَا تَفۡعَلُونَ","End":"١٢"},{"SurahNumber":82,"VerseNumber":13,"Text":"إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ","End":"١٣"},{"SurahNumber":82,"VerseNumber":14,"Text":"وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ","End":"١٤"},{"SurahNumber":82,"VerseNumber":15,"Text":"يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ","End":"١٥"},{"SurahNumber":82,"VerseNumber":16,"Text":"وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ","End":"١٦"},{"SurahNumber":82,"VerseNumber":17,"Text":"وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ","End":"١٧"},{"SurahNumber":82,"VerseNumber":18,"Text":"ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ","End":"١٨"},{"SurahNumber":82,"VerseNumber":19,"Text":"يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ","End":"١٩"}],"Translations":[]}