{"SurahNumber":82,"HasBismillah":true,"Verses":[{"SurahNumber":82,"VerseNumber":1,"Text":"إِذَا ٱلسَّمَآءُ ٱنفَطَرَتۡ","End":"١"},{"SurahNumber":82,"VerseNumber":2,"Text":"وَإِذَا ٱلۡكَوَاكِبُ ٱنتَثَرَتۡ","End":"٢"},{"SurahNumber":82,"VerseNumber":3,"Text":"وَإِذَا ٱلۡبِحَارُ فُجِّرَتۡ","End":"٣"},{"SurahNumber":82,"VerseNumber":4,"Text":"وَإِذَا ٱلۡقُبُورُ بُعۡثِرَتۡ","End":"٤"},{"SurahNumber":82,"VerseNumber":5,"Text":"عَلِمَتۡ نَفۡسٞ مَّا قَدَّمَتۡ وَأَخَّرَتۡ","End":"٥"},{"SurahNumber":82,"VerseNumber":6,"Text":"يَـٰٓأَيُّهَا ٱلۡإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلۡكَرِيمِ","End":"٦"},{"SurahNumber":82,"VerseNumber":7,"Text":"ٱلَّذِي خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ","End":"٧"},{"SurahNumber":82,"VerseNumber":8,"Text":"فِيٓ أَيِّ صُورَةٖ مَّا شَآءَ رَكَّبَكَ","End":"٨"},{"SurahNumber":82,"VerseNumber":9,"Text":"كَلَّا بَلۡ تُكَذِّبُونَ بِٱلدِّينِ","End":"٩"},{"SurahNumber":82,"VerseNumber":10,"Text":"وَإِنَّ عَلَيۡكُمۡ لَحَٰفِظِينَ","End":"١٠"},{"SurahNumber":82,"VerseNumber":11,"Text":"كِرَامٗا كَٰتِبِينَ","End":"١١"},{"SurahNumber":82,"VerseNumber":12,"Text":"يَعۡلَمُونَ مَا تَفۡعَلُونَ","End":"١٢"},{"SurahNumber":82,"VerseNumber":13,"Text":"إِنَّ ٱلۡأَبۡرَارَ لَفِي نَعِيمٖ","End":"١٣"},{"SurahNumber":82,"VerseNumber":14,"Text":"وَإِنَّ ٱلۡفُجَّارَ لَفِي جَحِيمٖ","End":"١٤"},{"SurahNumber":82,"VerseNumber":15,"Text":"يَصۡلَوۡنَهَا يَوۡمَ ٱلدِّينِ","End":"١٥"},{"SurahNumber":82,"VerseNumber":16,"Text":"وَمَا هُمۡ عَنۡهَا بِغَآئِبِينَ","End":"١٦"},{"SurahNumber":82,"VerseNumber":17,"Text":"وَمَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ","End":"١٧"},{"SurahNumber":82,"VerseNumber":18,"Text":"ثُمَّ مَآ أَدۡرَىٰكَ مَا يَوۡمُ ٱلدِّينِ","End":"١٨"},{"SurahNumber":82,"VerseNumber":19,"Text":"يَوۡمَ لَا تَمۡلِكُ نَفۡسٞ لِّنَفۡسٖ شَيۡـٔٗاۖ وَٱلۡأَمۡرُ يَوۡمَئِذٖ لِّلَّهِ","End":"١٩"}],"Translations":[{"Id":10,"LanguageCode":"ara","Name":"ara-quran-la","Author":"Quran Transliteration","Language":"Arabic","Description":"Arabic - Latinised","Direction":"ltr","Source":"http://tanzil.net","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"Itha alssamao infatarat"},{"SurahNumber":82,"VerseNumber":2,"Text":"Waitha alkawakibu intatharat"},{"SurahNumber":82,"VerseNumber":3,"Text":"Waitha albiharu fujjirat"},{"SurahNumber":82,"VerseNumber":4,"Text":"Waitha alqubooru buAAthirat"},{"SurahNumber":82,"VerseNumber":5,"Text":"AAalimat nafsun ma qaddamat waakhkharat"},{"SurahNumber":82,"VerseNumber":6,"Text":"Ya ayyuha alinsanu ma gharraka birabbika alkareemi"},{"SurahNumber":82,"VerseNumber":7,"Text":"Allathee khalaqaka fasawwaka faAAadalaka"},{"SurahNumber":82,"VerseNumber":8,"Text":"Fee ayyi sooratin ma shaa rakkabaka"},{"SurahNumber":82,"VerseNumber":9,"Text":"Kalla bal tukaththiboona bialddeeni"},{"SurahNumber":82,"VerseNumber":10,"Text":"Wainna AAalaykum lahafitheena"},{"SurahNumber":82,"VerseNumber":11,"Text":"Kiraman katibeena"},{"SurahNumber":82,"VerseNumber":12,"Text":"YaAAlamoona ma tafAAaloona"},{"SurahNumber":82,"VerseNumber":13,"Text":"Inna alabrara lafee naAAeemin"},{"SurahNumber":82,"VerseNumber":14,"Text":"Wainna alfujjara lafee jaheemin"},{"SurahNumber":82,"VerseNumber":15,"Text":"Yaslawnaha yawma alddeeni"},{"SurahNumber":82,"VerseNumber":16,"Text":"Wama hum AAanha bighaibeena"},{"SurahNumber":82,"VerseNumber":17,"Text":"Wama adraka ma yawmu alddeeni"},{"SurahNumber":82,"VerseNumber":18,"Text":"Thumma ma adraka ma yawmu alddeeni"},{"SurahNumber":82,"VerseNumber":19,"Text":"Yawma la tamliku nafsun linafsin shayan waalamru yawmaithin lillahi"}]},{"Id":34,"LanguageCode":"ara","Name":"ara-quranuthmanihaf1","Author":"Quran Uthmani Hafs No Diacritics","Language":"Arabic","Description":"Arabic","Direction":"rtl","Source":"https://qurancomplex.gov.sa/","Translations":[{"SurahNumber":82,"VerseNumber":1,"Text":"اذا السماء انفطرت"},{"SurahNumber":82,"VerseNumber":2,"Text":"واذا الكواكب انتثرت"},{"SurahNumber":82,"VerseNumber":3,"Text":"واذا البحار فجرت"},{"SurahNumber":82,"VerseNumber":4,"Text":"واذا القبور بعثرت"},{"SurahNumber":82,"VerseNumber":5,"Text":"علمت نفس ما قدمت واخرت"},{"SurahNumber":82,"VerseNumber":6,"Text":"يايها الانسن ما غرك بربك الكريم"},{"SurahNumber":82,"VerseNumber":7,"Text":"الذي خلقك فسوىك فعدلك"},{"SurahNumber":82,"VerseNumber":8,"Text":"في اي صورة ما شاء ركبك"},{"SurahNumber":82,"VerseNumber":9,"Text":"كلا بل تكذبون بالدين"},{"SurahNumber":82,"VerseNumber":10,"Text":"وان عليكم لحفظين"},{"SurahNumber":82,"VerseNumber":11,"Text":"كرام ا كتبين"},{"SurahNumber":82,"VerseNumber":12,"Text":"يعلمون ما تفعلون"},{"SurahNumber":82,"VerseNumber":13,"Text":"ان الابرار لفي نعيم"},{"SurahNumber":82,"VerseNumber":14,"Text":"وان الفجار لفي جحيم"},{"SurahNumber":82,"VerseNumber":15,"Text":"يصلونها يوم الدين"},{"SurahNumber":82,"VerseNumber":16,"Text":"وما هم عنها بغايبين"},{"SurahNumber":82,"VerseNumber":17,"Text":"وما ادرىك ما يوم الدين"},{"SurahNumber":82,"VerseNumber":18,"Text":"ثم ما ادرىك ما يوم الدين"},{"SurahNumber":82,"VerseNumber":19,"Text":"يوم لا تملك نفس لنفس شي ا والامر يوميذ لله"}]}]}