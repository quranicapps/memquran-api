{"SurahNumber":90,"HasBismillah":true,"Verses":[{"SurahNumber":90,"VerseNumber":1,"Text":"لَآ أُقۡسِمُ بِهَٰذَا ٱلۡبَلَدِ","End":"١"},{"SurahNumber":90,"VerseNumber":2,"Text":"وَأَنتَ حِلُّۢ بِهَٰذَا ٱلۡبَلَدِ","End":"٢"},{"SurahNumber":90,"VerseNumber":3,"Text":"وَوَالِدٖ وَمَا وَلَدَ","End":"٣"},{"SurahNumber":90,"VerseNumber":4,"Text":"لَقَدۡ خَلَقۡنَا ٱلۡإِنسَٰنَ فِي كَبَدٍ","End":"٤"},{"SurahNumber":90,"VerseNumber":5,"Text":"أَيَحۡسَبُ أَن لَّن يَقۡدِرَ عَلَيۡهِ أَحَدٞ","End":"٥"},{"SurahNumber":90,"VerseNumber":6,"Text":"يَقُولُ أَهۡلَكۡتُ مَالٗا لُّبَدًا","End":"٦"},{"SurahNumber":90,"VerseNumber":7,"Text":"أَيَحۡسَبُ أَن لَّمۡ يَرَهُۥٓ أَحَدٌ","End":"٧"},{"SurahNumber":90,"VerseNumber":8,"Text":"أَلَمۡ نَجۡعَل لَّهُۥ عَيۡنَيۡنِ","End":"٨"},{"SurahNumber":90,"VerseNumber":9,"Text":"وَلِسَانٗا وَشَفَتَيۡنِ","End":"٩"},{"SurahNumber":90,"VerseNumber":10,"Text":"وَهَدَيۡنَٰهُ ٱلنَّجۡدَيۡنِ","End":"١٠"},{"SurahNumber":90,"VerseNumber":11,"Text":"فَلَا ٱقۡتَحَمَ ٱلۡعَقَبَةَ","End":"١١"},{"SurahNumber":90,"VerseNumber":12,"Text":"وَمَآ أَدۡرَىٰكَ مَا ٱلۡعَقَبَةُ","End":"١٢"},{"SurahNumber":90,"VerseNumber":13,"Text":"فَكُّ رَقَبَةٍ","End":"١٣"},{"SurahNumber":90,"VerseNumber":14,"Text":"أَوۡ إِطۡعَٰمٞ فِي يَوۡمٖ ذِي مَسۡغَبَةٖ","End":"١٤"},{"SurahNumber":90,"VerseNumber":15,"Text":"يَتِيمٗا ذَا مَقۡرَبَةٍ","End":"١٥"},{"SurahNumber":90,"VerseNumber":16,"Text":"أَوۡ مِسۡكِينٗا ذَا مَتۡرَبَةٖ","End":"١٦"},{"SurahNumber":90,"VerseNumber":17,"Text":"ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُواْ وَتَوَاصَوۡاْ بِٱلصَّبۡرِ وَتَوَاصَوۡاْ بِٱلۡمَرۡحَمَةِ","End":"١٧"},{"SurahNumber":90,"VerseNumber":18,"Text":"أُوْلَـٰٓئِكَ أَصۡحَٰبُ ٱلۡمَيۡمَنَةِ","End":"١٨"},{"SurahNumber":90,"VerseNumber":19,"Text":"وَٱلَّذِينَ كَفَرُواْ بِـَٔايَٰتِنَا هُمۡ أَصۡحَٰبُ ٱلۡمَشۡـَٔمَةِ","End":"١٩"},{"SurahNumber":90,"VerseNumber":20,"Text":"عَلَيۡهِمۡ نَارٞ مُّؤۡصَدَةُۢ","End":"٢٠"}],"Translations":[]}