{"SurahNumber":86,"HasBismillah":true,"Verses":[{"SurahNumber":86,"VerseNumber":1,"Text":"وَٱلسَّمَآءِ وَٱلطَّارِقِ","End":"١"},{"SurahNumber":86,"VerseNumber":2,"Text":"وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ","End":"٢"},{"SurahNumber":86,"VerseNumber":3,"Text":"ٱلنَّجۡمُ ٱلثَّاقِبُ","End":"٣"},{"SurahNumber":86,"VerseNumber":4,"Text":"إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ","End":"٤"},{"SurahNumber":86,"VerseNumber":5,"Text":"فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ","End":"٥"},{"SurahNumber":86,"VerseNumber":6,"Text":"خُلِقَ مِن مَّآءٖ دَافِقٖ","End":"٦"},{"SurahNumber":86,"VerseNumber":7,"Text":"يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ","End":"٧"},{"SurahNumber":86,"VerseNumber":8,"Text":"إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ","End":"٨"},{"SurahNumber":86,"VerseNumber":9,"Text":"يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ","End":"٩"},{"SurahNumber":86,"VerseNumber":10,"Text":"فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ","End":"١٠"},{"SurahNumber":86,"VerseNumber":11,"Text":"وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ","End":"١١"},{"SurahNumber":86,"VerseNumber":12,"Text":"وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ","End":"١٢"},{"SurahNumber":86,"VerseNumber":13,"Text":"إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ","End":"١٣"},{"SurahNumber":86,"VerseNumber":14,"Text":"وَمَا هُوَ بِٱلۡهَزۡلِ","End":"١٤"},{"SurahNumber":86,"VerseNumber":15,"Text":"إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا","End":"١٥"},{"SurahNumber":86,"VerseNumber":16,"Text":"وَأَكِيدُ كَيۡدٗا","End":"١٦"},{"SurahNumber":86,"VerseNumber":17,"Text":"فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا","End":"١٧"}],"Translations":[]}