{"SurahNumber":104,"HasBismillah":true,"Verses":[{"SurahNumber":104,"VerseNumber":1,"Text":"وَيۡلٞ لِّكُلِّ هُمَزَةٖ لُّمَزَةٍ","End":"١"},{"SurahNumber":104,"VerseNumber":2,"Text":"ٱلَّذِي جَمَعَ مَالٗا وَعَدَّدَهُۥ","End":"٢"},{"SurahNumber":104,"VerseNumber":3,"Text":"يَحۡسَبُ أَنَّ مَالَهُۥٓ أَخۡلَدَهُۥ","End":"٣"},{"SurahNumber":104,"VerseNumber":4,"Text":"كَلَّاۖ لَيُنۢبَذَنَّ فِي ٱلۡحُطَمَةِ","End":"٤"},{"SurahNumber":104,"VerseNumber":5,"Text":"وَمَآ أَدۡرَىٰكَ مَا ٱلۡحُطَمَةُ","End":"٥"},{"SurahNumber":104,"VerseNumber":6,"Text":"نَارُ ٱللَّهِ ٱلۡمُوقَدَةُ","End":"٦"},{"SurahNumber":104,"VerseNumber":7,"Text":"ٱلَّتِي تَطَّلِعُ عَلَى ٱلۡأَفۡـِٔدَةِ","End":"٧"},{"SurahNumber":104,"VerseNumber":8,"Text":"إِنَّهَا عَلَيۡهِم مُّؤۡصَدَةٞ","End":"٨"},{"SurahNumber":104,"VerseNumber":9,"Text":"فِي عَمَدٖ مُّمَدَّدَةِۭ","End":"٩"}],"Translations":[]}