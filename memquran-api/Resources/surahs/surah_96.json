{"SurahNumber":96,"HasBismillah":true,"Verses":[{"SurahNumber":96,"VerseNumber":1,"Text":"ٱقۡرَأۡ بِٱسۡمِ رَبِّكَ ٱلَّذِي خَلَقَ","End":"١"},{"SurahNumber":96,"VerseNumber":2,"Text":"خَلَقَ ٱلۡإِنسَٰنَ مِنۡ عَلَقٍ","End":"٢"},{"SurahNumber":96,"VerseNumber":3,"Text":"ٱقۡرَأۡ وَرَبُّكَ ٱلۡأَكۡرَمُ","End":"٣"},{"SurahNumber":96,"VerseNumber":4,"Text":"ٱلَّذِي عَلَّمَ بِٱلۡقَلَمِ","End":"٤"},{"SurahNumber":96,"VerseNumber":5,"Text":"عَلَّمَ ٱلۡإِنسَٰنَ مَا لَمۡ يَعۡلَمۡ","End":"٥"},{"SurahNumber":96,"VerseNumber":6,"Text":"كَلَّآ إِنَّ ٱلۡإِنسَٰنَ لَيَطۡغَىٰٓ","End":"٦"},{"SurahNumber":96,"VerseNumber":7,"Text":"أَن رَّءَاهُ ٱسۡتَغۡنَىٰٓ","End":"٧"},{"SurahNumber":96,"VerseNumber":8,"Text":"إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجۡعَىٰٓ","End":"٨"},{"SurahNumber":96,"VerseNumber":9,"Text":"أَرَءَيۡتَ ٱلَّذِي يَنۡهَىٰ","End":"٩"},{"SurahNumber":96,"VerseNumber":10,"Text":"عَبۡدًا إِذَا صَلَّىٰٓ","End":"١٠"},{"SurahNumber":96,"VerseNumber":11,"Text":"أَرَءَيۡتَ إِن كَانَ عَلَى ٱلۡهُدَىٰٓ","End":"١١"},{"SurahNumber":96,"VerseNumber":12,"Text":"أَوۡ أَمَرَ بِٱلتَّقۡوَىٰٓ","End":"١٢"},{"SurahNumber":96,"VerseNumber":13,"Text":"أَرَءَيۡتَ إِن كَذَّبَ وَتَوَلَّىٰٓ","End":"١٣"},{"SurahNumber":96,"VerseNumber":14,"Text":"أَلَمۡ يَعۡلَم بِأَنَّ ٱللَّهَ يَرَىٰ","End":"١٤"},{"SurahNumber":96,"VerseNumber":15,"Text":"كَلَّا لَئِن لَّمۡ يَنتَهِ لَنَسۡفَعَۢا بِٱلنَّاصِيَةِ","End":"١٥"},{"SurahNumber":96,"VerseNumber":16,"Text":"نَاصِيَةٖ كَٰذِبَةٍ خَاطِئَةٖ","End":"١٦"},{"SurahNumber":96,"VerseNumber":17,"Text":"فَلۡيَدۡعُ نَادِيَهُۥ","End":"١٧"},{"SurahNumber":96,"VerseNumber":18,"Text":"سَنَدۡعُ ٱلزَّبَانِيَةَ","End":"١٨"},{"SurahNumber":96,"VerseNumber":19,"Text":"كَلَّا لَا تُطِعۡهُ وَٱسۡجُدۡۤ وَٱقۡتَرِب۩","End":"١٩"}],"Translations":[]}