{
  "AzureKeyVaultName": "<pass in secrets or env var when deploying>",
  "AzureUserManagedIdentityClientId": "<pass in secrets or env var when deploying>",
  "ConnectionStrings": {
    "Redis": "dockerhost:6379"
  },
  "HealthCheckSettings": {
    "LocalFiles": {
      "Name": "Local JSON Files",
      "Tag": "LocalFiles",
      "Enabled": true,
      "TimeOut": "00:00:15"
    },
    "JsDelivr": {
      "Name": "JsDelivr CDN",
      "Tag": "JsDelivr",
      "Enabled": true,
      "TimeOut": "00:00:15"
    },
    "Redis": {
      "Name": "Redis Cache",
      "Tag": "Redis",
      "Enabled": false,
      "TimeOut": "00:00:15"
    }
  },
  "ContentDeliverySettings": {
    "ContentDeliveryType": "JsDelivr", // Local, JsDelivr
    "CachingSettings": {
      "CacheType": "None", // None, Memory, Hybrid
      "InitialCachingEnabled": false,
      "SlidingExpiration": false,
      "CacheDurationTimeSpan": "02:00:00:00",
      "EvictCachingEnabled": false
    }
  },
  "Clients": {
    "JsDelivrService": {
      "Enabled": true,
      "BaseUrl": "https://cdn.jsdelivr.net",
      "Version": "v0.0.63",
      "DefaultTimeout": "00:00:15",
      "IncrementalBackoffFactorSeconds": 5,
      "UserName": "",
      "Password": ""
    }
  },
  "AwsHostSettings": {
    "ProfileName": "",
    "AccessKey": "",
    "SecretKey": "",
    "ServiceUrl": "http://dockerhost:4566",
    "RegionEndpoint": "eu-west-2"
  },
  "AwsTopicSettings": {
    "WebUpdateTopicSettings": {
      "WorkerName": "memquran_api_web_update_topic_worker",
      "Source": "memquran_web_update_topic1_v1",
      "WithSubscribedQueues": [
        "memquran_web_update_queue1_v1",
        "memquran_web_update_queue2_v1"
      ],
      "SubscribeToSource": "memquran_web_update_queue1_v1",
      "NumberOfInstances": 1,
      "IsFifoQueue": true,
      "IsFifoContentBasedDeduplication": true,
      "BuildWithErrorQueue": true,
      "ReceiveMaximumNumberOfMessages": 10
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "MemQuran.Api": "Information",
      "MemQuran.Infrastructure.Caching": "Information",
      "Topica": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Hosting": "Warning",
      "Microsoft.AspNetCore.Routing": "Warning",
      "Microsoft.AspNetCore.Mvc": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Extensions.Caching.StackExchangeRedis.RedisCache": "Information"
    },
    "Console": {
      "FormatterName": "CustomTimePrefixingFormatter",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "yyyy-MM-dd HH:mm:ss.ffff ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
    }
  },
  "SeqSettings": {
    "ServerUrl": "http://dockerhost:5341",
    "OpenTelemetryTraceIngestUrl": "http://dockerhost:5341/ingest/otlp/v1/traces",
    "ApiKey": "<Get from secrets -> SeqSettings:ApiKey>",
    "MinimumLevel": "Trace",
    "LevelOverride": {
      "Default": "Warning"
    }
  },
  "JaegerSettings": {
    "Endpoint": "http://dockerhost:4317"
  },
  "BetterStackSettings": {
    "IngestBaseUrl": "<Get from secrets -> BetterStackSettings:IngestBaseUrl>",
    "BearerToken": "<Get from secrets -> BetterStackSettings:BearerToken>",
  },
  "AllowedHosts": "*"
}
